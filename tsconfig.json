{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/WelcomeScreen.tsx",
    "app/_layout.tsx",
    "app/App.js",
    "src/services/Api.ts",
    "src/styles/Colors.ts",
    "src/appwrite/AppwriteConfig.ts"
, "src/styles/Fonts.ts"  ]
}
